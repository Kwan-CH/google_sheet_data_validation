{
    "String": {
        "rule": "isText",
        "param": {
            "allowEmpty": false
        }
    },
    "Number": {
        "rule": "isNumeric",
        "param": {
            "allowEmpty": false,
            "allowNegative": false,
            "minimum": 0,
            "maximum": 100
        }
    },
    "Email": {
        "rule": "isEmail",
        "param": {
            "allowEmpty": false
        }
    },
    "Date": {
        "rule": "isDate",
        "param": {
            "allowEmpty": false
        }
    },
    "Duplicate": {
        "rule": "isDuplicate",
        "param": {
            "allowEmpty": false
        }
    },
    "Prefix": {
        "rule": "isPrefix",
        "param": {
            "allowEmpty": false,
            "prefix": "PRE"
        }
    },
    "Option": {
        "rule": "isInOption",
        "param": {
            "allowEmpty": false,
            "options": "kiwi, orange, apple"
        }
    },
    "ASCII": {
        "rule": "isAlphanumeric",
        "param": {
            "allowEmpty": false
        }
    }
}